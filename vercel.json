{
  "builds": [
    {
      "src": "streamlit_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "streamlit_app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task",
    "STREAMLIT_SERVER_HEADLESS": "true",
    "STREAMLIT_SERVER_PORT": "8501"
  },
  "functions": {
    "streamlit_app.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    }
  }
}
